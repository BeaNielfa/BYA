<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:background="@drawable/fondo_margaritas"
                                                   android:id="@+id/mujerLayout"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   tools:context=".ui.catalogoUsuario.CatalogoMujerFragment">

    <View
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:background="@color/black"
            android:id="@+id/view1"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/view2" />

    <View
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:background="@color/black"
            android:id="@+id/view2"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/view1" />

    <View
            android:layout_width="2dp"
            android:layout_height="match_parent"
            android:background="@color/black"
            android:id="@+id/view3"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

    <ImageView
            android:src="@drawable/ic_camiseta"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:id="@+id/imgCatalogoMujerCamiseta"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/view3"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="24dp" />

    <TextView
            android:text="@string/camiseta"
            android:textColor="@color/black"
            android:textStyle="bold"
            android:textSize="20sp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tvCatalogoMujerCamiseta"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/view3"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/imgCatalogoMujerCamiseta" />

    <ImageView
            android:src="@drawable/ic_blusa"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:id="@+id/imgCatalogoMujerBlusa"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/view3"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="24dp" />

    <TextView
            android:text="@string/blusa"
            android:textColor="@color/black"
            android:textStyle="bold"
            android:textSize="20sp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tvCatalogoMujerBlusa"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/view3"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/imgCatalogoMujerBlusa" />

    <ImageView
            android:src="@drawable/ic_vestido"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:id="@+id/imgCatalogoMujerVestido"
            android:layout_marginTop="24dp"
            app:layout_constraintTop_toBottomOf="@+id/view2"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/view3" />

    <TextView
            android:text="@string/vestido"
            android:textColor="@color/black"
            android:textStyle="bold"
            android:textSize="20sp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tvCatalogoMujerVestido"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/view3"
            app:layout_constraintTop_toBottomOf="@+id/imgCatalogoMujerVestido"
            android:layout_marginTop="8dp" />

    <ImageView
            android:src="@drawable/ic_jeansm"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:id="@+id/imgCatalogoMujerJeans"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/view3"
            android:layout_marginTop="24dp"
            app:layout_constraintTop_toBottomOf="@+id/view2" />

    <TextView
            android:text="@string/jeans"
            android:textColor="@color/black"
            android:textStyle="bold"
            android:textSize="20sp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tvCatalogoMujerJeans"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/view3"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/imgCatalogoMujerJeans" />

    <ImageView
            android:src="@drawable/ic_bolso"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:id="@+id/imgCatalogoMujerAccesorio"
            app:layout_constraintEnd_toStartOf="@+id/view3"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="24dp"
            app:layout_constraintTop_toBottomOf="@+id/view1" />

    <TextView
            android:text="@string/accesorio"
            android:textColor="@color/black"
            android:textStyle="bold"
            android:textSize="20sp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tvCatalogoMujerAccesorio"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/view3"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/imgCatalogoMujerAccesorio" />

    <ImageView
            android:src="@drawable/ic_falda"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:id="@+id/imgCatalogoMujerFalda"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/view3"
            android:layout_marginTop="24dp"
            app:layout_constraintTop_toBottomOf="@+id/view1" />

    <TextView
            android:text="@string/falda"
            android:textColor="@color/black"
            android:textStyle="bold"
            android:textSize="20sp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tvCatalogoMujerFalda"
            app:layout_constraintStart_toEndOf="@+id/view3"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/imgCatalogoMujerFalda" />


</androidx.constraintlayout.widget.ConstraintLayout>